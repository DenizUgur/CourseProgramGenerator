{
  "name": "courseprogramgen",
  "productName": "Course Program Generator",
  "description": "Automated program generator with alternatives",
  "version": "2.0.0",
  "private": true,
  "author": "Deniz Uğur <deniz343@gmail.com>",
  "copyright": "© 2020, Deniz",
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.deniz.courseprogramgen",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "artifactName": "${productName}-${os}.${ext}",
    "publish": null
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "CI=true npm test -- --coverage",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "preelectron-pack": "CI=true npm run build",
    "electron-pack": "electron-builder --macos --c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "electron-is-dev": "^1.2.0",
    "fs": "0.0.1-security",
    "moment": "^2.27.0",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "xlsx": "^0.16.2"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^9.0.4",
    "electron-builder": "^22.7.0",
    "wait-on": "^5.0.1",
    "typescript": "~3.7.2",
    "webpack": "^4.42.0"
  }
}
