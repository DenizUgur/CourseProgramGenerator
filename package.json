{
  "name": "courseprogramgen",
  "productName": "Course Program Generator",
  "description": "Automated program generator with alternatives",
  "version": "1.0.0",
  "private": true,
  "author": "Deniz Uğur <deniz343@gmail.com>",
  "copyright": "© 2020, Deniz",
  "homepage": "http://www.denizdaking.com",
  "main": "app/background.js",
  "build": {
    "appId": "com.deniz.courseprogramgen",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "artifactName": "${productName}-${os}.${ext}",
    "publish": null
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test --display=none",
    "unit": "electron-mocha temp/specs.js --renderer --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test --display=none && webpack --config=build/webpack.e2e.config.js --env=test --display=none",
    "e2e": "mocha temp/e2e.js --require source-map-support/register --timeout 10000",
    "coverage": "npm run unit && nyc mocha temp/specs.js --renderer --require source-map-support/register",
    "coverage-report": "nyc report --reporter=html",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "prerelease": "webpack --config=build/webpack.app.config.js --env=production",
    "release": "npm run test && npm run prerelease && electron-builder"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "fs-jetpack": "^2.1.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "xlsx": "^0.14.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^1.0.0",
    "electron": "2.0.8",
    "electron-builder": "22.2.0",
    "electron-mocha": "^6.0.4",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.6",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2",
    "serialize-javascript": ">=2.1.1"
  }
}
